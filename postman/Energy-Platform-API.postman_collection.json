{
	"info": {
		"_postman_id": "energy-platform-api-001",
		"name": "Energy Platform API",
		"description": "API collection for enrgy-BE (Gas/Energy maintenance platform).\n\n**Setup:**\n1. Set `baseUrl` (e.g. `http://localhost:4000`).\n2. Run **Auth > Login** with email/password; the access token is saved automatically.\n3. All other requests use `Authorization: Bearer {{accessToken}}`.",
		"schema": "https://schema.getpostman.com/json/collection/v2.1.0/collection.json"
	},
	"variable": [
		{ "key": "baseUrl", "value": "http://localhost:4000" },
		{ "key": "accessToken", "value": "" }
	],
	"auth": {
		"type": "bearer",
		"bearer": [{ "key": "token", "value": "{{accessToken}}", "type": "string" }]
	},
	"item": [
		{
			"name": "Auth",
			"item": [
				{
					"name": "Register",
					"request": {
						"method": "POST",
						"header": [{ "key": "Content-Type", "value": "application/json" }],
						"body": {
							"mode": "raw",
							"raw": "{\n  \"organizationName\": \"My Company\",\n  \"organizationType\": \"SERVICE_PROVIDER\",\n  \"email\": \"admin@example.com\",\n  \"password\": \"Password123!\",\n  \"fullName\": \"Admin User\",\n  \"phone\": \"+201000000000\"\n}"
						},
						"url": "{{baseUrl}}/api/auth/register",
						"description": "Register new organization (SERVICE_PROVIDER or FUEL_STATION). Org starts as PENDING.",
						"auth": { "type": "noauth" }
					}
				},
				{
					"name": "Login",
					"event": [
						{
							"listen": "test",
							"script": {
								"exec": [
									"const json = pm.response.json();",
									"if (json.success && json.data && json.data.accessToken) {",
									"  pm.collectionVariables.set('accessToken', json.data.accessToken);",
									"}"
								],
								"type": "text/javascript"
							}
						}
					],
					"request": {
						"method": "POST",
						"header": [{ "key": "Content-Type", "value": "application/json" }],
						"body": {
							"mode": "raw",
							"raw": "{\n  \"email\": \"admin@techserve.com\",\n  \"password\": \"Password123!\"\n}"
						},
						"url": "{{baseUrl}}/api/auth/login",
						"description": "Login with email/password. Saves accessToken to collection variable.",
						"auth": { "type": "noauth" }
					}
				},
				{
					"name": "Refresh Token",
					"request": {
						"method": "POST",
						"header": [{ "key": "Content-Type", "value": "application/json" }],
						"body": {
							"mode": "raw",
							"raw": "{\n  \"refreshToken\": \"{{refreshToken}}\"\n}"
						},
						"url": "{{baseUrl}}/api/auth/refresh",
						"description": "Get new access token using refresh token.",
						"auth": { "type": "noauth" }
					}
				},
				{
					"name": "Me",
					"request": {
						"method": "GET",
						"url": "{{baseUrl}}/api/auth/me",
						"description": "Current user profile with organization, roles, permissions."
					}
				}
			]
		},
